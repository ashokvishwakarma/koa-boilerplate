{
  "name": "koa-boilerplate",
  "description": "An easy getting started boilerplate using KoaJS, Mongoose, Kue and Koa-Router",
  "version": "1.0.0",
  "author": "Ashok Vishwakarma <akvlko@gmail.com>",
  "keywords": [
    "koa",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "async",
    "es7"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ashokvishwakarma/koa-boilerplate.git"
  },
  "homepage": "https://github.com/ashokvishwakarma/koa-boilerplate.git#readme",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon src/ --exec babel-node",
    "build": "babel src -d dist",
    "lint": "eslint ./src",
    "test": "npm run lint && npm run mocha",
    "prod": "npm run build && node dist/",
    "mocha": "./node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill"
  },
  "dependencies": {
    "async-busboy": "^0.6.2",
    "babel-polyfill": "^6.5.0",
    "dotenv": "^4.0.0",
    "es6-promisify": "^4.1.0",
    "file-type": "^7.4.0",
    "fluent-ffmpeg": "^2.1.2",
    "glob": "^7.1.1",
    "jsonwebtoken": "^7.2.1",
    "koa": "^2.0.0-alpha.7",
    "koa-body": "^2.5.0",
    "koa-docs": "^2.1.0",
    "koa-helmet": "^2.0.0",
    "koa-json-filter": "0.0.1",
    "koa-router": "^7.0.0",
    "koa-send": "^4.1.2",
    "koa2-cors": "^2.0.5",
    "kue": "^0.11.6",
    "mime-types": "^2.1.17",
    "mongoose": "^4.4.3",
    "mongoose-paginate": "^5.0.3",
    "nodemailer": "^4.4.1",
    "read-chunk": "^2.1.0",
    "redis": "^2.8.0",
    "sharp": "^0.18.4"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.8.1",
    "should": "^11.1.2",
    "supertest": "^2.0.1"
  }
}
